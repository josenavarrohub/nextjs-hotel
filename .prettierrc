{
	"plugins": ["prettier-plugin-tailwindcss"],
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"printWidth": 100,
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"endOfLine": "lf",
	"embeddedLanguageFormatting": "auto",
	"singleAttributePerLine": false
}
  